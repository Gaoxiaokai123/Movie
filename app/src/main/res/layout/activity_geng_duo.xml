<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.activity.GengDuoActivity"
    android:background="#141931">

    <include layout="@layout/head"></include>


    <LinearLayout
        android:layout_marginTop="40dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

    <android.support.design.widget.TabLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        app:tabTextColor="@android:color/white"
        android:id="@+id/tab_gengduo_tab"></android.support.design.widget.TabLayout>

        <android.support.v4.view.ViewPager
            android:id="@+id/tab_gengduo_frag"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="12"></android.support.v4.view.ViewPager>

    </LinearLayout>

</RelativeLayout>